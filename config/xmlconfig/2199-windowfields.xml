<?xml version="1.0" encoding="utf-8"?>
<ameisen-node>
<config>
	<listtype>sort</listtype>
	<overview>fieldlabel,windowname,name_db,xtype,store,mandatory,value,readonly,hidden,showenptycombobutton,showselectallcombobutton,tab,permission</overview>
	<pagetitle>#fieldlabel#</pagetitle>
	<usehistory>no</usehistory>
	<keytype>id</keytype>
	<hide>false</hide>
</config>
<editfields> 
	<field>
		<name>windowname</name>
		<type>genericelement</type>
		<element>windows</element>
		<required>yes</required>
		<multiple>no</multiple>
	</field>
	<field>
		<name>fieldlabel</name> 
		<type>textshort</type>
		<required>yes</required>
	</field>
	<field>
		<name>name_db</name> 
		<type>textshort</type>
		<required>yes</required>
	</field>
	<field>
		<name>xtype</name>
		<type>set</type>
		<items>textfield,combobox,textarea,numberfield,checkbox,htmleditor,fileuploadfield,displayfield,datefield,image,datetime,hiddenfield,file,timefield</items>
		<default>textfield</default>
		<required>yes</required>
		<multiple>no</multiple>
	</field>
	<field>
		<name>mehrfachauswahl</name>
		<type>set</type>
		<items>ja,nein</items>
		<default>nein</default>
		<required>yes</required>
		<multiple>no</multiple>
	</field>
	<field>
		<name>mehrfachauswahl_convert</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>store</name> 
		<type>textlong</type>
		<required>no</required>
	</field>
	<field>
		<name>displayfield</name> 
		<type>textshort</type>
		<required>no</required>
	</field>
	<field>
		<name>valuefield</name> 
		<type>textshort</type>
		<required>no</required>
	</field>
	<field>
		<name>mandatory</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>maxlength</name> 
		<type>integer</type>
		<required>no</required>
	</field>
	<field>
		<name>emptytext</name> 
		<type>textshort</type>
		<required>no</required>
	</field>
	<field>
		<name>querymode</name>
		<type>set</type>
		<items>local,remote</items>
		<default>local</default>
		<required>no</required>
		<multiple>no</multiple>
	</field>
	<field>
		<name>height</name> 
		<type>integer</type>
		<required>no</required>
	</field>
	<field>
		<name>value</name> 
		<type>textshort</type>
		<required>no</required>
	</field>
	<field>
		<name>readonly</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>hidden</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>showenptycombobutton</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>showselectallcombobutton</name> 
		<type>bool</type>
		<required>no</required>
	</field>
	<field>
		<name>flags</name>
		<type>set</type>
		<items>dontshowinreadmodus,readonlyineditmodus,disableineditmodus,donothidetrigger</items>
		<required>no</required>
		<multiple>yes</multiple>
	</field>
	<field>
		<name>tab</name>
		<type>set</type>
		<items>ebene1,ebene2,ebene3</items>
		<default></default>
		<required>no</required>
		<multiple>no</multiple>
	</field>
	<field>
		<name>permission</name>
		<type>set</type>
		<items>gruppenleiter,admin,eigene</items>
		<default></default>
		<required>no</required>
		<multiple>no</multiple>
		<hidden>true</hidden>
	</field>
</editfields> 
</ameisen-node>